@{
    SiteSettings siteSettings = ViewData.Get<SiteSettings>("SiteSettings", null);
}
<div class="tn-footer-legal">
    @if (siteSettings != null)
    {
        <p>
            @Html.Raw(siteSettings.Links)
        </p>
    }
    <p>
        Powered by <a href="http://www.jinhusns.com" target="_blank">近乎</a> v4.2 ©2005-2014 Tunynet Inc.
        @if (siteSettings != null)
        {
            @Html.Raw(siteSettings.BeiAnScript)
            @Html.Raw(siteSettings.StatScript)
        }
    </p>
</div>
<div class="tn-return-to-top" id="to_top">
    <a href="#" class="tn-button tn-corner-all tn-button-secondary tn-button-hollow tn-button-icon-only">
        <span class="tn-icon tn-icon-top"></span><span class="tn-button-text">返回顶部</span></a>
</div>